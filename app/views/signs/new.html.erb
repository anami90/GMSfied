  </head>

  <body>

    <div class="container">

      <div class ="form-signin">
        <h2 class="form-signin-heading">Please sign in</h2>
        
<%= form_for(:sign, :url => signs_path) do |f| %> 

  <div class="field">
    <%= f.label :email %><br/>
    <%= f.text_field :email %>
   </div>
   <div class="field">
   <%= f.label :password %><br/>
   <%= f.password_field :password %> 
   </div>
   <div class="actions">
   <%= f.submit "Sign in"%>
   <br/>
   <br/>
   <p><h1>forgot pword</h1><p>
   </div>
  </div> 
 </div>  
<%end%>


   
   
